PARALLEL
mpirun -np 1 $tcmc square5.order2.hdf -solver=petsc -tol=1.e-12 >! tcmc.parallel.out


SERIAL
$tcmc square5.order2.hdf -solver=petsc -tol=1.e-12 >! tcmc.serial.out


