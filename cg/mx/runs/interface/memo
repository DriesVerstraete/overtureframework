-- TEST split-square interface

cgmx -noplot interface -g=twoSquaresSplitInterfacee2.order4.ng3 -eps1=.25 -tf=100 -tp=.1 -ic=pmic -diss=0.5  -left=leftSquare -right=rightSquare -bc=d -go=og

-- OK but Ex has error blip on interface
cgmx -noplot interface -g=twoSquaresSplitInterfacee2.order4.ng3 -eps2=.25 -tf=100 -tp=.1 -ic=pmic -diss=0.5  -left=leftSquare -right=rightSquare -bc=d -go=og

eps1=.25 OK
cgmx interface -g=twoBoxesSplitInterfaceGride222.order4 -eps1=.25 -tp=.01 -ic=pmic -tf=20 -tp=.1 -bc=d -diss=.5 -left=leftBox -right=rightBox -interfaceEquationOption=0 -go=halt

eps2=.25 OK -- but Ex has error blip on interface
cgmx interface -g=twoBoxesSplitInterfaceGride222.order4 -eps2=.25 -tp=.01 -ic=pmic -tf=20 -tp=.1 -bc=d -diss=.5 -left=leftBox -right=rightBox -interfaceEquationOption=0 -go=halt

eps1=2.25  OK with -diss=.5
cgmx interface -g=twoBoxesSplitInterfaceGride222.order4 -eps1=2.25 -tp=.01 -ic=pmic -tf=20 -tp=.1 -bc=d -diss=.5 -left=leftBox -right=rightBox -interfaceEquationOption=0 -go=halt


